{
    "name": "sanmai/cdek-info",
    "type": "library",
    "description": "Набор справочников для СДЭК PHP SDK",
    "keywords": [
        "CDEK",
        "sdk",
        "СДЭК"
    ],
    "homepage": "https://github.com/sanmai/cdek-info",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com",
            "homepage": "https://www.alexeykopytko.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "phpdocumentor/reflection-docblock": "^4.3",
        "sanmai/pipeline": "^0.3.2 || ^2.0 || ^3.0.1"
    },
    "conflict": {},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "infection/infection": ">=0.10",
        "phan/phan": "^1.1",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": ">=0.10",
        "phpunit/phpunit": "^6.5 || ^7.4",
        "vimeo/psalm": ">=3.0.16"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v0.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "CdekSDK\\Reference\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CdekSDK\\Reference\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/sanmai/cdek-info/issues",
        "chat": "https://t.me/phpcdeksdk"
    }
}
